{
  "name": "printing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "build:pages": "npx @cloudflare/next-on-pages",
    "deploy:pages": "npx wrangler@latest pages deploy .vercel/output/static --project-name printing-frontend",
    "dev:pages": "npx @cloudflare/next-on-pages --watch",
    "preview:pages": "npx wrangler@latest pages dev .vercel/output/static --compatibility-date=2025-09-15 --compatibility-flag=nodejs_compat",
    "dev:worker": "wrangler dev -c cf-worker/wrangler.toml"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.883.0",
    "@aws-sdk/s3-request-presigner": "^3.883.0",
    "clsx": "^2.0.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.470.0",
    "next": "15.5.2",
    "pdfjs-dist": "^4.0.379",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.2.3",
    "tailwind-merge": "^2.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^9.0.7",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^3.114.14"
  },
  "overrides": {
    "esbuild": "0.14.47"
  }
}
